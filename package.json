{
	"name": "website",
	"version": "0.1.0",
	"homepage": "https://www.ryan-ak.com",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"all": "^0.0.0",
		"gh-pages": "^6.2.0",
		"html-react-parser": "^5.1.18",
		"react": "^17.0.2",
		"react-animated-css": "^1.2.1",
		"react-dom": "^17.0.2",
		"react-ga": "^3.3.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^5.0.1",
		"sass": "^1.80.5",
		"web-vitals": "^1.0.1"
	},
	"scripts": {
		"start": "react-scripts --openssl-legacy-provider start",
		"build": "react-scripts --openssl-legacy-provider build",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"test": "react-scripts --openssl-legacy-provider test",
		"test:coverage": "react-scripts --openssl-legacy-provider test --coverage --watchAll=false",
		"test:ci": "react-scripts --openssl-legacy-provider test --coverage --watchAll=false --ci",
		"test:a11y": "react-scripts --openssl-legacy-provider test --testNamePattern=\"Accessibility\"",
		"test:unit": "react-scripts --openssl-legacy-provider test --testNamePattern=\"Unit\"",
		"test:routing": "react-scripts --openssl-legacy-provider test --testNamePattern=\"Routing\"",
		"test:debug": "react-scripts --openssl-legacy-provider test --debug",
		"eject": "react-scripts eject",
		"copy": "cp ./CNAME ./build/CNAME",
		"lint": "eslint . --fix",
		"format": "prettier --write ."
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/plugin-proposal-private-property-in-object": "7.21.11",
		"@eslint/js": "^9.14.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.2",
		"globals": "^15.11.0",
		"jest-axe": "^10.0.0"
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"overrides": {
		"node-fetch": "^3.3.2",
		"nth-check": "2.1.1"
	}
}